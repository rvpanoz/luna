  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "devtron": "^1.4.0",
    "electron-debug": "^2.0.0",
    "electron-log": "^2.2.17",
    "electron-store": "^2.0.0",
