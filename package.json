    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
