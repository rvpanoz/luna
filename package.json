    "prop-types": "^15.7.2",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "redux-react-hook": "^4.0.1",
    "rxjs": "^6.6.2",
    "source-map-support": "^0.5.19",
    "typeface-roboto": "0.0.75",
